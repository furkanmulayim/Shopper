<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/item_food_category_back"
    android:layout_width="wrap_content"
    android:layout_height="45dp"
    android:layout_marginStart="5dp"
    android:layout_marginTop="12dp"
    android:layout_marginEnd="5dp"
    android:elevation="2dp"
    android:layout_marginBottom="12dp"
    android:background="@drawable/category_item_white"
    android:orientation="horizontal">


    <ImageView
        android:id="@+id/logo"
        android:layout_width="22dp"
        android:layout_height="22dp"
        android:foregroundGravity="center"
        android:layout_gravity="center"
        android:layout_marginStart="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/deneee"
        app:layout_constraintEnd_toEndOf="@+id/deneee"
        app:layout_constraintStart_toStartOf="@+id/deneee"
        app:layout_constraintTop_toTopOf="@+id/deneee" />


    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="10dp"
        android:text="furkan"
        android:fontFamily="@font/inter_semibold"
        android:textColor="@color/normal_text"
        android:textSize="12sp" />
</LinearLayout>